@model PROYECTO_TP_PNT1.Models.Alojamiento

@{
    ViewData["Title"] = "Delete";
}

<div class="row" id="detalles">
    <h1 class="titulo-cliente">ELIMINAR ALOJAMIENTO</h1>
    <hr />
    <div>
        <dl class="row" id="detalle-row">
            <dt class="col-sm-5">
                -  @Html.DisplayNameFor(model => model.nombre) :
            </dt>
            <dd class="col-sm-5">
                @Html.DisplayFor(model => model.nombre)
            </dd>
            <dt class="col-sm-5">
                -  @Html.DisplayNameFor(model => model.direccion) :
            </dt>
            <dd class="col-sm-5">
                @Html.DisplayFor(model => model.direccion)
            </dd>
            <dt class="col-sm-5">
                - @Html.DisplayNameFor(model => model.cantHabitacionesDisponibles) :
            </dt>
            <dd class="col-sm-5">
                @Html.DisplayFor(model => model.cantHabitacionesDisponibles)
            </dd>
            <dt class="col-sm-5">
                -  @Html.DisplayNameFor(model => model.tipo) :
            </dt>
            <dd class="col-sm-5">
                @Html.DisplayFor(model => model.tipo)
            </dd>
            <form asp-action="Delete" id="botones-form">

                <input type="submit" value="Eliminar" class="btn btn-danger bg-danger" />
                <a asp-action="Index" class="btn bg-dark boton-volver">Volver</a>
            </form>
        </dl>
    </div>
    <div id="resact">
        @ViewBag.AlojamientoConReserva
    </div>
</div>
